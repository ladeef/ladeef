var chklogin;
var glob;
function chk_login(a)
{ 
    //alert("aaa");
	
	var valid=true;
	var err="";
	glob=a;
	//alert(document.getElementById("login_email"+a).value);
	if(document.getElementById("login_email"+a).value=="")
	{
		//document.getElementById("errlog_email"+a).innerHTML="(Enter Email)";
		err="Enter Username";
		valid=false;
	}
	
	if(document.getElementById("login_password"+a).value=="")
	{
		//document.getElementById("errlog_pass"+a).innerHTML="Enter Password";
		if(err=="")
			err="Enter Password";
		else
			err=err+"<br/>Enter Password";
		valid=false;
	}
			if(valid)
			{	
				document.getElementById("errlog_email"+a).innerHTML="";
				WaitOn();
				//var remb_me;
				var stremail=document.getElementById("login_email"+a).value;
				var strpass=document.getElementById("login_password"+a).value;
				//alert(stremail);		
				var url="login_proc.php?sid=" + Math.random();				
				var param="loginemail="+stremail+"&loginpass="+ strpass;
				//alert(param);
				chklogin=GetXmlHttpObject(checklogin)
				chklogin.open("POST", url , true)
				chklogin.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
				chklogin.setRequestHeader("Content-length", param.length);
				chklogin.setRequestHeader("Connection", "close");
				chklogin.send(param)
			}
			else
			{
			document.getElementById("errlogajax"+a).innerHTML=err;
			document.getElementById("errlogajax"+a).style.display="block";
			}
	
}
function checklogin() 
{ 
	if (chklogin.readyState==4 || chklogin.readyState=="complete")
	{ 
		var ajaxresponse=chklogin.responseText;
		//alert(ajaxresponse);
		if(ajaxresponse==1)
		{
		  //alert("aa");
		  window.location="profile.php";
			
		}
		
		else
		{
		//alert("ccc");
		document.getElementById("errlogajax"+glob).innerHTML=ajaxresponse;
		document.getElementById("errlogajax"+glob).style.display="block";
		}
		WaitOff();		
	} 
} 


</script>
